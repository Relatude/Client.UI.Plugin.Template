{
    "name": "@company/my_plugin",
    "version": "1.0.0",
    "description": "Relatude UI Plugin Template",
    "scripts": {
        "start": "parcel src/start.html --no-cache --port 80",
        "build": "parcel build src/plugin.ts --no-cache --no-source-maps --out-dir build/release --global MyGlobalPluginName",
        "build_debug": "parcel build src/plugin.ts --no-cache --no-minify --out-dir build/debug --global MyGlobalPluginName"
    },
    "scriptsComments": {
        "start": "For development. Starts a dev build in watch mode and a local HTTP server",
        "build": "Build minified files for publishing your plugin",
        "build_debug": "Build a debuggable version of your plugin with source maps and no minification"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "browserslist": [
        "last 1 Chrome version"
    ],
    "externals": "./externals.js",
    "dependencies": {
        "@parcel/logger": "^2.0.0-alpha.3",
        "@relatude/api": "^2.1.2",
        "@relatude/colors": "^2.1.2",
        "@relatude/common": "^2.1.2",
        "@relatude/core": "^2.1.2",
        "@relatude/icons": "^2.1.2",
        "font-awesome": "^4.7.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "regenerator-runtime": "^0.13.7"
    },
    "devDependencies": {
        "@types/react": "^16.9.46",
        "@types/react-dom": "^16.9.8",
        "postcss-modules": "^1.5.0",
        "typescript": "^3.6.4",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-externals": "^0.5.1"
    }
}
